{
  "scripts": {
    "build": "npx --ignore-existing --quiet rimraf dist/* && webpack --mode production",
    "build:watch": "webpack --mode production --watch",
    "clean:build": "npx --ignore-existing --quiet rimraf dist/**/*",
    "clean:build-deps": "npx --ignore-existing --quiet rimraf dist/node_modules",
    "clean:deps": "npx --ignore-existing --quiet rimraf package-lock.json pnpm-lock.yaml node_modules",
    "lint": "npm run lint:prettier && npm run lint:compile && npm run lint:eslint && npm run lint:stylelint",
    "lint:compile": "tsc -p tsconfig.json --noEmit --noErrorTruncation --pretty",
    "lint:eslint": "eslint \"**/*.{js,jsx}\" --fix",
    "lint:prettier": "prettier \"./**/*.{css,html,js,json,jsx,md,scss,yaml}\" --write",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "release": "standard-version",
    "start": "webpack-dev-server",
    "test": "jest --maxWorkers 1",
    "test:changed": "jest --maxWorkers 1 --onlyChanged",
    "test:coverage": "jest --maxWorkers 1 --coverage"
  },
